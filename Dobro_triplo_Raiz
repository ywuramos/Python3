n = int(input('Digite um numero: '))  # DOBRO, TRIPLO, RAIZ
do = n * 2
tri = n * 3
raiz = n**(1/2)
print('Dobro = {}, Triplo = {}, Raiz quadrada = {:.2f} '.format(do, tri, raiz))

